@model IEnumerable<sivel_proyecto_bad115.Models.TALLERES>

@{
    ViewBag.Title = "Index";
}

<h2>TALLERES</h2>

<p>
    @Html.ActionLink("Crear nuevo taller", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.NOMBRE)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.NOMBRE)
        </td>
        <td>
            @Html.ActionLink("Editar", "Edit", new { id=item.ID_TALLER }) |
            @Html.ActionLink("Detalles", "Details", new { id=item.ID_TALLER }) |
            @Html.ActionLink("Eliminar", "Delete", new { id=item.ID_TALLER })
        </td>
    </tr>
}

</table>
