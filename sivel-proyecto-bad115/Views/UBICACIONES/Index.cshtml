@model IEnumerable<sivel_proyecto_bad115.Models.UBICACIONES>

@{
    ViewBag.Title = "Index";
}

<h2>UBICACIONES</h2>

<p>
    @Html.ActionLink("Crear nueva ubicacion", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.NIVEL)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.NUMERO_PARQUEO)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.BODEGAS.NOMBRE)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.NIVEL)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.NUMERO_PARQUEO)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.BODEGAS.NOMBRE)
        </td>
        <td>
            @Html.ActionLink("Editar", "Edit", new { id=item.ID_UBICACION }) |
            @Html.ActionLink("Detalles", "Details", new { id=item.ID_UBICACION }) |
